{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "impliedStrict": true
        }
    },
    "extends": [ "eslint:recommended", "plugin:react/recommended" ],
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "no-mixed-spaces-and-tabs": "warn",
      "no-unused-vars": "warn"
    },
    "plugins": [
      "react"
    ]
}
